<?xml version="1.0" encoding="UTF-8"?>
<table-config>
  <table name="example-table">
    <columns>
      <column name="id" type="number" width="100" sortable="true"/>
      <column name="name" type="string" width="200" sortable="true"/>
      <column name="date" type="date" width="150"/>
      <column name="status" type="string" width="120"/>
    </columns>
    <pagination pageSize="10" currentPage="1"/>
    <sorting column="id" direction="asc"/>
  </table>
</table-config>
